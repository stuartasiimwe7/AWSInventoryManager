{
  "dashboard": {
    "id": null,
    "title": "Business Intelligence",
    "tags": ["business", "analytics"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "User Registrations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(user_registrations_total[1h])",
            "legendFormat": "Registrations/hour",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Users/hour"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "API Usage by Service",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(api_calls_total[5m])",
            "legendFormat": "{{service}} - {{endpoint}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Calls/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Inventory Levels",
        "type": "graph",
        "targets": [
          {
            "expr": "inventory_levels",
            "legendFormat": "{{product_id}} ({{category}})",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Units"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Sales Volume by Category",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(sales_volume_total[1h])",
            "legendFormat": "{{product_category}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Sales/hour"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Total Users",
        "type": "singlestat",
        "targets": [
          {
            "expr": "sum(increase(user_registrations_total[30d]))",
            "legendFormat": "Total Users",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "short",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Daily Active Users",
        "type": "singlestat",
        "targets": [
          {
            "expr": "sum(increase(user_registrations_total[1d]))",
            "legendFormat": "DAU",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "short",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "API Success Rate",
        "type": "singlestat",
        "targets": [
          {
            "expr": "(1 - (rate(api_calls_total{status=~\"5..\"}[5m]) / rate(api_calls_total[5m]))) * 100",
            "legendFormat": "Success Rate %",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "percent",
        "thresholds": "95,99",
        "colorBackground": true,
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Low Stock Alerts",
        "type": "singlestat",
        "targets": [
          {
            "expr": "count(inventory_levels < 10)",
            "legendFormat": "Low Stock Items",
            "refId": "A"
          }
        ],
        "valueName": "current",
        "format": "short",
        "thresholds": "1,5",
        "colorBackground": true,
        "colorValue": false,
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 16
        }
      },
      {
        "id": 9,
        "title": "Top Products by Sales",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sales_volume_total)",
            "legendFormat": "{{product_category}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 10,
        "title": "API Endpoint Usage",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, rate(api_calls_total[1h]))",
            "legendFormat": "{{service}} - {{endpoint}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        }
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "refresh": "1m"
  }
}
